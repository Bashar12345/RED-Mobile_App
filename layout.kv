#: import images_path kivymd.images_path
<CustomTexInput@TextInput>
	 background_normal: f'{images_path}transperent.png'
	 background_active: self.background_normal
	 foreground_color: 0,0,0,.5
	 hint_test_color: app.theme_cls.disabled_hit_text_color
	 padding:"8dp","12dp","8dp",0
	 cursor_color:0,0,0,.2
	 size_hint_y:None
	 height:"38dp"
	 canvas.after:
	 	 Color:
		  	 rgba:0,0, 0.005, 0.07
			   RoundRectangle:
			   	 pos:self.pos
					size:self.size
					radious: [8,]
# CustomTexInput:
# 	 hint_text:'Enter search'

<UI>:
	 MDScreen:
	 	 name:"homepage"
		  MDBoxLayout:
		  	 pos_hint: {'top': 1.0}
			   size_hint_y:None
			   height:'60dp'
			   spacing:'10dp'
			   padding:'12dp',0,'12dp',0
			   FitImage:
			   	 source:'images/05.jpg'
					mipimap: True
					size_hint: None, None
					size:'40dp','40dp'
					pos_hint: {'center_y': 0.5}
					radius:[30,]
			   MDLabel:
			   	 text:'RED'
					font_style:'H5'
					bold:True
			   MDIconButton:
			   	 icon:'hand-heart'
					pos_hint: {'center_y': 0.5}
					radius:[30,]
			   MDIconButton:
			   	 icon:'logout'
					pos_hint: {'center_y': 0.5}
					radius:[30,]
		  MDBoxLayout:
		  	 size_hint_y:.15
			 ScrollView: