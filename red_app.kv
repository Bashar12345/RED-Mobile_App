#:kivy 2.1.0
Main_layout:

	 

<Main_layout>:
	 orientation:'vertical'
	 size: root.width, root.height
	 padding: 5
	 spacing: '10dp'
	 #Line:
	 MDToolbar:
	 	 title: "RED"
		  right_action_items:[["icon : 'data/logo/kivy-icon-256.png'",lambda x: app.login()]]
		#   MDIconButton:
		#   	 icon: "data/logo/kivy-icon-256.png"
	 GridLayout:
	 	 cols:1
		  #rows:5
		  #Line:
		  header:
		  Button:
		  	 on_press: root.on_click()
		  	 text:"Test Button"
			   size_hint: None,None
			   width: '80dp'
			   height: '45dp'
		  MDLabel:
		  	 text:root.btn_txt
			   font_name: 'fonts/lcd.ttf'
			   color: 1,.5,.5,1
		  Button:
		  	 text:"Menu3"
			   size_hint: None,None
			   width: '50dp'
			   height: '45dp'
		  Button:
		  	 text:"Menu2"
			   size_hint: None,None
			   width: '50dp'
			   height: '45dp'

<main_screen>:
     Button:
	 	 text:'Hello'
		  size: '40dp', '40dp'
		  pos: '100dp', '200dp'
     Button:
	 	 text:'Hello2'
		  size: '40dp', '40dp'
		  pos: '200dp', '200dp'
     Label:
	 	 text:'Hello'
		  size: '40dp', '40dp'
		  pos: '300dp', '200dp'
		  color: 1, 0,0,1
